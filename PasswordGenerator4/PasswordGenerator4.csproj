<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <UseWPF>True</UseWPF>
    <ApplicationIcon>Password Desktop Icon.ico</ApplicationIcon>
    <Win32Resource></Win32Resource>
    <AssemblyName>PasswordGenerator</AssemblyName>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Resources\PasswordGeneratorLogo.ico" />
    <None Remove="Resources\PasswordGeneratorLogo.png" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Resources\PasswordGeneratorLogo.ico" />
    <Resource Include="Resources\PasswordGeneratorLogo.png" />
  </ItemGroup>

</Project>
